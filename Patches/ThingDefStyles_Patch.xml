<Patch>

<!-- Morbid Style -->

	<Operation Class="PatchOperationAdd">
		<success>Normal</success>
		<xpath>/Defs/StyleCategoryDef[defName = "Morbid"]/thingDefStyles</xpath>
		<value>
			<li>
				<thingDef>Pew</thingDef>
				<styleDef>TST_Morbid_Pew</styleDef>
			</li>
			<li>
				<thingDef>TST_CushionedPew</thingDef>
				<styleDef>TST_Morbid_CushionedPew</styleDef>
			</li>
		</value>
	</Operation>

